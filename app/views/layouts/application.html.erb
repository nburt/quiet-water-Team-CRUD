<!DOCTYPE html>
<html>
<head>
  <title>Media Manager</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <META HTTP-EQUIV="refresh" CONTENT="<%= @meta %>">
</head>
<body>
<header>
  <div class="headerfootercontainer">
    <div class="headfootalign">
      <div id="headertitle">
        <a href="/"><h1>Media Manager</h1></a>
      </div>
    </div>
    <div id="nav_links"><a href="/pictures">all pictures</a> | <a href="/videos">all videos</a>
    </div>
  </div>
</header>
<div id="bodycontainer">
  <div id="bodycontent">
    <%= yield %>
  </div>
</div>
<footer>
  <div class="headerfootercontainer">
    <div class="headfootalign">
      <div id="footertext">Not copyrighted at all</div>
    </div>
  </div>
</footer>
</body>

</html>